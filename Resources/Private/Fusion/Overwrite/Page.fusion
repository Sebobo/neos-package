prototype(Neos.Neos:Page) {
    goeBodyScript = '<script src="https://app.goessential.com/app/client.bundle.js"></script>'
    goeBodyScript.@position = 'before closingBodyTag'
    goeBodyScript.@if.render = ${q(documentNode).children('[instanceof Neos.Neos:ContentCollection]').find('[instanceof GoE.Neos:Mixin.Content]').is()}
}
